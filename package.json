{
  "name": "app",
  "version": "1.1.0",
  "description": "",
  "author": "Seogyu Yeriel Kim",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "main": "dist/main.js",
  "packageManager": "pnpm@8.1.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "mig:up": "typeorm-ts-node-esm migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test mocha -r ts-node/esm test/**/**/**/*.spec.ts --exit",
    "test:file": "NODE_ENV=test mocha -r ts-node/esm --exit",
    "test:cov": "NODE_ENV=test nyc mocha -r ts-node/esm test/**/**/**/*.spec.ts --exit",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18 < 19",
    "npm": ">=8 <9",
    "pnpm": ">=8 <9"
  },
  "nyc": {
    "branches": "&gt;80",
    "lines": "&gt;80",
    "functions": "&gt;80",
    "statements": "&gt;80",
    "exclude": [
      "src/infra",
      "test",
      "src/constants",
      "**/*.strategy.ts",
      "**/*.entity.ts",
      "**/*zod*"
    ]
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^1.8.0",
    "@anatine/zod-openapi": "^1.11.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5",
    "cache-manager-redis-store": "^3.0.1",
    "chai": "^4.3.7",
    "compression": "^1.7.4",
    "connect-redis": "^7.0.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "ioredis": "^5.3.1",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.8.0",
    "openapi3-ts": "^3.1.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.8.2",
    "winston-slack-webhook-transport-ts": "^1.2.7",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/compression": "^1.7.2",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "sqlite3": "^5.1.4",
    "supertest": "^6.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.0.0"
  }
}
